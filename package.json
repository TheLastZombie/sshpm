{
  "name": "sshpm",
  "version": "1.0.0-rc.1",
  "description": "The SSH profile manager.",
  "keywords": [
    "config",
    "configuration",
    "connection",
    "cyberduck",
    "electerm",
    "filezilla",
    "kitty",
    "manager",
    "mremoteng",
    "oclif",
    "openssh",
    "profile",
    "putty",
    "rclone",
    "remmina",
    "ssh",
    "winscp",
    "zoc"
  ],
  "homepage": "https://git.io/sshpm",
  "bugs": "https://github.com/TheLastZombie/sshpm/issues",
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "TheLastZombie",
    "url": "https://eric.jetzt/"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "main": "src/index.js",
  "bin": {
    "sshpm": "./bin/run"
  },
  "repository": "github:TheLastZombie/sshpm",
  "scripts": {
    "dist": "oclif-dev pack",
    "lint": "prettier --write . && standard --fix",
    "postpack": "rimraf oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "start": "node ./bin/run",
    "test": "jest",
    "version": "oclif-dev readme && git add README.md"
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.1",
    "@oclif/plugin-not-found": "^1.2.4",
    "cli-ux": "^5.5.1",
    "iconv-lite": "^0.6.2",
    "jszip": "^3.6.0",
    "oclif-plugin-completion": "^0.6.0",
    "open": "^8.0.2",
    "rage-edit": "^1.2.0",
    "ssh2": "^1.1.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "jest": "^27.0.4",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "standard": "^16.0.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "jest": {
    "rootDir": "test",
    "testMatch": [
      "**/*.js"
    ]
  },
  "oclif": {
    "commands": "./src/commands",
    "bin": "sshpm",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "oclif-plugin-completion"
    ]
  },
  "standard": {
    "env": [
      "jest"
    ]
  }
}
